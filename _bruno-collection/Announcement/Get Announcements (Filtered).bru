meta {
  name: Get Announcements (Filtered)
  type: http
  seq: 9
}

get {
  url: {{GATEWAY_URL}}/api/announcements?ownerId=1&status=PUBLISHED
  body: none
  auth: bearer
}

auth:bearer {
  token: {{ACCESS_TOKEN}}
}

script:post-response {
  if (res.status === 200) {
    console.log(`✅ Retrieved ${Array.isArray(res.body) ? res.body.length : 0} filtered announcements`);
  } else {
    console.error('❌ Failed to get filtered announcements:', res.status);
  }
}